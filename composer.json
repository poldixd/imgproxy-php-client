{
    "name": "poldixd/imgproxy",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Poltmann",
            "email": "dev@nils.li"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^7.1|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.4|^9.0|^9.5"
    },
    "autoload": {
        "psr-4": {
            "poldixd\\ImgProxy\\": "src/poldixd/ImgProxy/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "poldixd\\ImgProxy\\Providers\\ImgProxyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
